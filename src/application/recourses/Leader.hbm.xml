<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="application.recourses.Faculty" table="faculties" catalog="university">
        <id name="ID" type="java.lang.Integer">
            <column name="ID" />
        </id>
        <property name="facultyName" type="string">
            <column name="faculty-name" length="32" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>